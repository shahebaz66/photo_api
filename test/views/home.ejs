<body class="text-center">
    <div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">
        <header class="masthead mb-auto">
            <div class="inner">
                <h3 class="masthead-brand">Cover</h3>
                

            </div>
        </header>
        <h3 class="masthead-brand"><%=status%></h3>
        <main role="main" class="inner cover">

            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#photo">
                Add Photos
            </button>
            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#video">
                Add videos
            </button>
            <a href="/getPhoto" class="btn btn-primary">See all photos</a>
            <a href="/getVideo" class="btn btn-primary">See all videos</a>
        </main>

        <footer class="mastfoot mt-auto">
            <div class="inner">
                <p>admin panel for <a href="https://getbootstrap.com/">ramakanth</a>, by <a
                        href="https://twitter.com/mdo">@mdo</a>.</p>
            </div>
        </footer>
    </div>
    <!-- Trigger the modal with a button -->



    <!-- The Modal -->


    <!-- Modal -->
    <div class="modal fade" id="photo" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel" style="color: black;">Add Photos </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form action="/addphoto" method="post" enctype="multipart/form-data">

                        <input type="hidden" name="type" value="photo">
                        <div class="form-group">

                            <select class="form-control" name="category" id="sel1">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                            </select>
                        </div>
                        <input type="file" name="photo" class="form-control" multiple>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                            <button type="submit" class="btn btn-primary">Save changes</button>
                        </div>
                    </form>
                </div>

            </div>
        </div>
    </div>

    <div class="modal fade" id="video" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel" style="color: black;">Add Videos </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form action="/addvideo" method="post" enctype="multipart/form-data">

                        <input type="hidden" name="type" value="video">
                        <div class="form-group">

                            <select class="form-control" name="category" id="sel1">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                            </select>
                        </div>
                        <input type="file" name="video" class="form-control" multiple>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                            <button type="submit" class="btn btn-primary">Save changes</button>
                        </div>
                    </form>
                </div>

            </div>
        </div>
    </div>

    <!-- Modal -->

